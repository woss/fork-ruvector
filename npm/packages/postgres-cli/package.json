  "description": "Command-line interface for RuVector PostgreSQL extension - advanced AI vector database",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "ruvector-pg": "dist/cli.js",
    "rvpg": "dist/cli.js"

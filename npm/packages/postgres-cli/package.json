    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ruvector",
    "postgres",
    "postgresql",
    "vector",
    "database",
    "cli",

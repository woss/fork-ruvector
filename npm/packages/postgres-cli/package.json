    "dev": "tsc --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "node --test tests/*.test.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
